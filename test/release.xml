<?xml version="1.0" encoding="UTF-8"?>
<assembly>
    <id></id>
    <baseDirectory>test</baseDirectory>
    <formats>
        <format>tar.gz</format>
    </formats>
    <fileSets>
        <fileSet>
            <includes>
                <include>startup.sh</include>
                <include>*.jar</include>
            </includes>
            <excludes>
                <exclude>**/src/**</exclude>
                <exclude>**/target/**</exclude>
                <exclude>**/.*/**</exclude>
            </excludes>
        </fileSet>
    </fileSets>
    <dependencySets>
        <dependencySet>
            <outputDirectory>/lib</outputDirectory>
            <!--如果全部需要，把下面includes删除 -->
            <includes>
                <include>com.google.guava:guava</include>
            </includes>
        </dependencySet>
    </dependencySets>
</assembly>
